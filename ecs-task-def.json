{
  "family": "blackbeard-extension",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::066866130858:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "blackbeard-extension",
      "image": "ghcr.io/tdupoiron-org/blackbeard-extension:latest",
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:eu-west-3:066866130858:secret:tdupoiron-github-packages-read"
      },
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
